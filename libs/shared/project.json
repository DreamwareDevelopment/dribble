{
  "name": "shared",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project shared --web",
  "targets": {
    "type-gen": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma generate --schema ./src/prisma/schema.prisma",
        "cwd": "libs/shared"
      }
    },
    "migrate-dev": {
      "executor": "nx:run-commands",
      "outputs": [],
      "dependsOn": [],
      "options": {
        "command": "npx prisma migrate dev --schema ./src/prisma/schema.prisma --name adding_email_subscriptions",
        "cwd": "libs/shared"
      }
    },
    "migrate": {
      "executor": "nx:run-commands",
      "outputs": [],
      "dependsOn": ["type-gen"],
      "options": {
        "command": "npx prisma migrate deploy --schema ./src/prisma/schema.prisma",
        "cwd": "libs/shared"
      }
    }
  }
}
